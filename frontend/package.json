{
  "name": "react-mesto-auth",
  "version": "0.1.0",
  "description": "Learning project",
  "author": "Nikita Nemykin",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "npm run build && scp -r ./build/* kluk@kluk.students.nomoreparties.space:/home/kluk/react-mesto-api-full/frontend-news"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.js?(x)"
        ],
        "rules": {
          "no-underscore-dangle": [
            "error",
            {
              "allow": [
                "_id"
              ]
            }
          ],
          "linebreak-style": [
            "error",
            "windows"
          ],
          "indent": [
            "error",
            4
          ],
          "no-console": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://kluk42.github.io/mesto-react/",
  "devDependencies": {
    "gh-pages": "^3.1.0"
  }
}
